# Environment Configuration for NaviQ Conductor Integration
# Copy this file to .env and update the values as needed

# Conductor Server Configuration
CONDUCTOR_SERVER_URL=http://localhost:8080
CONDUCTOR_KEY_ID=
CONDUCTOR_KEY_SECRET=

# Development Settings
NODE_ENV=development

# Database Configuration (if using Orkes Cloud)
# CONDUCTOR_AUTH_TOKEN=your_auth_token_here

# NaviQ Backend Integration
NAVIQ_BACKEND_URL=http://localhost:3000
NAVIQ_BACKEND_API_KEY=

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_METRICS=true

# Worker Configuration
WORKER_POLLING_INTERVAL=1000
WORKER_CONCURRENCY=10
WORKER_DOMAIN=naviq

# Timeouts (in milliseconds)
DEFAULT_WORKFLOW_TIMEOUT=300000
DEFAULT_TASK_TIMEOUT=60000
NAVIGATION_WORKFLOW_TIMEOUT=30000
BUILDING_CREATION_TIMEOUT=1800000

# Redis Configuration (if using custom Redis)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Elasticsearch Configuration (if using custom ES)
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
