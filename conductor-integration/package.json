{
	"name": "naviq-conductor-integration",
	"version": "1.0.0",
	"description": "Orkes Conductor integration for NaviQ navigation platform",
	"main": "integration/conductor.service.js",
	"type": "module",
	"scripts": {
		"start": "node integration/conductor.service.js",
		"dev": "nodemon integration/conductor.service.js",
		"test": "echo \"Error: no test specified\" && exit 1",
